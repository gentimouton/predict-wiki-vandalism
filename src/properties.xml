<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>

	<property>
			<name>base.path</name>
			<value>hdfs:///user/hadoop/data/</value>
	</property>
	
	<property>
			<name>output.file.name</name>
			<value>output</value>
	</property>
				
	<property>
			<name>smote.class.name</name>
			<value>buyGold</value>
	</property>
	
	<property>
			<name>num.folds</name>
			<value>10</value>
	</property>
	
	<property>
			<name>num.repeats</name>
			<value>10</value>
	</property>
	
	<property>
			<name>num.trees</name>
			<value>1000</value>
	</property>
	
	<property>
			<name>step.tree</name>
			<value>1</value>
	</property>

	<property>
			<name>smote.rate</name>
			<value>100</value>
	</property>
	
	<property>
			<name>smote.rate.step</name>
			<value>100</value>
	</property>
	
	<property>
			<name>smote.k</name>
			<value>5</value>
	</property>
	
	<property>
			<name>smote.k.step</name>
			<value>2</value>
	</property>
	
</configuration>
